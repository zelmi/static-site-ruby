<h1>Professional Activities</h1>
<div>
	<div>
		<h2>Program Committee Member</h2>
			<ul>
      			<% data.profAct.progComMem.each do |e| %>
      				<li>
      					<%= e.title %>" "<%= e.year %>
      				</li>
      			<% end %>
      		</ul>
	</div>
	<div>
		<h2>Invited Talks</h2>
			<% data.profAct.invTalks.each do |e| %>
				<label>
					<%= e.talk %>
				</label>
				<ul>
					<li>
						"LocatiAon: "<%= e.location %>
					</li>
					<li>
						"Date: "<%= e.date %>
					</li>
				</ul>
			<% end %>
	</div>
	<div>
		<h2>Other Activities</h2>
			<label>Editorial Board</label>
				<ul>
					<% data.profAct.otherAct.editBoard.each do |e| %>
						<li>
							<%= e.activity %>
						</li>
					<% end %>
				</ul>
			<label>Organizer</label>
				<ul>
					<% data.profAct.otherAct.organizer.each do |e| %>
						<li>
							<%= e.name %>
						</li>
					<% end %>
				</ul>
			<label>Tutorials</label>
				<ul>
					<% data.profAct.otherAct.tutorials.each do |e| %>
						<li>
							<%= e.name %>
						</li>
					<% end %>
				</ul>
			<label>Miscellaneous</label>
				<% data.profAct.otherAct.actMisc.each do |e| %>
					<label>
						<%= e.name %>
					</label>
					<ul>
						<li>
							<%= e.link %>
						</li>
					</ul>
				<% end %>
	</div>
</div>
